{
  "expo": {
    "name": "Classic Car Care",
    "slug": "Garageapp",
    "version": "6.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "jsEngine": "hermes",
    "platforms": ["ios", "android"],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "6.0.0"  // Incremented build number for iOS
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.unitx.Garageapp",
      "versionCode": 6  // Incremented version code for Android
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a6a090ba-617f-47cc-92fc-ba43fc61b38d"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
